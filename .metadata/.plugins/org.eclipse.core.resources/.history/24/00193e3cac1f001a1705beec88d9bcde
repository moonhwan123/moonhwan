// 이스케이프문자 "\n" , "\r" , "\t"


public class Exam_05 {
	
	public static void main(String[] args) {
		
		System.out.print("*\n");
		System.out.print("**\r");
		System.out.print("***\n");
		System.out.print("****\n");
		System.out.print("*****\n");
		
		
		
		
		
		
	}

}

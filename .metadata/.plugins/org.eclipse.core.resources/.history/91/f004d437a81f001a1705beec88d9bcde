//표준 입출력 (입력 - 키보드 , 출력 - 모니터)
//표준 출력
public class Exam_01 {

	public static void main(String[] args) {
		
		// System = 클래스 , out = 속성 , print = 메서드
		
		
		
		System.out.print(90); // 정수형 상수
		System.out.print("seoul"); // 문자열
		System.out.print("홍길동"); // 문자열
		System.out.print('a'); // 문자형 상수
		//System.out.print(a); // 변수
		
		System.out.println(); // print() + line() -> 줄바꿈
		
		
		
		
		
		

	}

}

	<div class="container">
		<div class="board_view">
			<h2>${vo.subject }</h2>
			<p class="info"><span class="user"></span> | ${vo.regdate.substring(0,10) } | <i class="fa fa-eye"></i> ${vo.readcnt }</p>
			<div class="board_body">
			
			${vo.contents }

			</div>
			<div class="prev_next">
				<a href="" class="btn_prev"><i class="fa fa-angle-left"></i>
